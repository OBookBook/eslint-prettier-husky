{
  "env": {
    "browser": true, //window, alert, document
    "node": true, //process
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module" //import/export
  },
  "rules": {
    "no-unused-vars": [
      "off"
    ],
    "linebreak-style": [
      "off"
    ]
  },
  "extends": [
    "airbnb-base"
  ]
}
